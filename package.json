  "main": "server.js",
  "scripts": {
    "test": "test",
    "start": "npm run server",
	"prettier": "prettier",
    "server": "node ./build/server.js",
    "client": "node start-client.js",
    "build-tsc": "tsc -p ./",
    "start-tsc": "npm run build-tsc -- -w",
    "localserver2": "node -r dotenv/config ./build/server.js",
    "localserver": "nodemon -r dotenv/config ./build/server.js",
    "local": "concurrently \"npm run localserver\" \"npm run client\" \"npm run start-tsc\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
